{
  "cli": {
    "version": ">= 16.1.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true,
      "channel": "production",
      "distribution": "store",
      "android": {
        "credentialsSource": "remote",
        "buildType": "app-bundle"
      },
      "env": {
        "SUPABASE_URL": "@SUPABASE_URL",
        "SUPABASE_ANON_KEY": "@SUPABASE_ANON_KEY"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
