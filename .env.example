# Supabase Configuration
EXPO_PUBLIC_SUPABASE_URL=your_supabase_url
EXPO_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key

# Branch.io Configuration
BRANCH_LIVE_KEY=key_live_xxxxxxxxxxxxxxxxxxxxxxxxx
BRANCH_TEST_KEY=key_test_xxxxxxxxxxxxxxxxxxxxxxxxx
BRANCH_APP_DOMAIN=your-app-name.app.link
BRANCH_ALT_DOMAIN=your-app-name-alternate.app.link

# Google OAuth Configuration
EXPO_PUBLIC_GOOGLE_WEB_CLIENT_ID=your_web_client_id
EXPO_PUBLIC_GOOGLE_ANDROID_CLIENT_ID=your_android_client_id
EXPO_PUBLIC_GOOGLE_IOS_CLIENT_ID=your_ios_client_id

# Sentry Configuration (Optional)
SENTRY_DSN=your_sentry_dsn
SENTRY_ORG=your_sentry_org
SENTRY_PROJECT=your_sentry_project

# App Configuration
EXPO_PUBLIC_APP_NAME=EduShorts
EXPO_PUBLIC_APP_SLUG=edushorts
EXPO_PUBLIC_APP_SCHEME=edushort

# Deep Linking Configuration
EXPO_PUBLIC_APP_SCHEME=edushort
EXPO_PUBLIC_WEB_URL=https://edushortlinks.netlify.app

# Note: Replace all placeholder values with actual values for your environment
# For development, copy this file to .env and update the values
# Do not commit the actual .env file to version control
