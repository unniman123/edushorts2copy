# Supabase Configuration
EXPO_PUBLIC_SUPABASE_URL=your-project-url
EXPO_PUBLIC_SUPABASE_KEY=your-anon-key
EXPO_PUBLIC_SUPABASE_STORAGE_URL=your-storage-url

# Feature Flags
EXPO_PUBLIC_USE_MOCK=false            # Set to true for development with mock data
EXPO_PUBLIC_USE_MOCK_FALLBACK=true    # Set to true to use mock data when real data fails
EXPO_PUBLIC_USE_MOCK_AUTH=false       # Set to true to bypass authentication in development
